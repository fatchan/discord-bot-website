	{
		"name": "Misc",
		"color": "#33cc33",
		"commands": [
			{
				"name": "ow",
				"args": "<battletag> <mode> [hero] [category]",
				"description": "get overwatch stats. battletags are case sensitive. Can either pick a battletag and mode for a quick overview, or add a hero and category for in-depth stats. Modes: competitive|quickplay Categories: specific|combat|assists|best|average|game|awards|miscellaneous",
				"examples": [
					">ow xQc#11273 competitive",
					">ow xQc#11273 quickplay",
					">ow xQc#11273 competitive winston combat",
					">ow xQc#11273 competitive overall game"
				]
			},
			{
				"name": "urban",
				"args": "<search term>",
				"description": "get urban dictionary definition",
				"examples": [
					">urban potato",
					">urban discord"
				]
			},
			{
				"name": "minecraft",
				"args": "",
				"description": "see minecraft server status",
				"examples": [
					">minecraft"
				]
			},
			{
				"name": "emoji",
				"args": "<text>",
				"description": "convert text to emojis. english letters, numbers, ! and ? supported. Max 80 characters.",
				"examples": [
					">emoji hello world!"
				]
			},
			{
				"name": "vaporwave",
				"args": "<text>",
				"description": "convert text to vaporwave.",
				"examples": [
					">vaporwave hello world!"
				]
			},
			{
				"name": "omegalul",
				"args": "<text>",
				"description": "replace o's with OMEGALUL emojis.",
				"examples": [
					">omegalul hello world!"
				]
			},
			{
				"name": "clap",
				"args": "<text>",
				"description": "Add claps between words.",
				"examples": [
					">clap this command is cool"
				]
			}
		]
	},
	{
		"name": "Perms",
		"color": "#e6e6e6",
		"commands": [
			{
				"name": "perms",
				"args": "<@mention> <command> <true/false>",
				"description": "change permissions for a user or role to a command",
				"examples": [
					">perms @somebody stop true",
					">perms @somebody hangman false"
				]
			},
			{
				"name": "perms",
				"args": "<@mention>admin",
				"description": "grant a user or role admin permissions (all command true). anyone with perms command permissions can use this.",
				"examples": [
					">perms @somebody admin"
				]
			},
			{
				"name": "perms",
				"args": "<@mention>show",
				"description": "see all permissions of a user or role",
				"examples": [
					">perms @somebody show"
				]
			},
			{
				"name": "perms",
				"args": "<command> default <true/false>",
				"description": "change default permissions for a command",
				"examples": [
					">perms play default false",
					">perms stop default true"
				]
			},
			{
				"name": "perms",
				"args": "[@mention | command] wipe",
				"description": "wipe permissions for a user, role, command or everything",
				"examples": [
					">perms @somebody wipe",
					">perms stop wipe",
					">perms wipe"
				]
			},
			{

				"name": "perms",
				"args": "<command>",
				"description": "view current command permissions",
				"examples": [
					">perms list",
					">perms stop"
				]
			}
		]
	}
]
